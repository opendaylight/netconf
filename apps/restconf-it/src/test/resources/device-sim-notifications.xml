<notifications>
    <!-- https://docs.opendaylight.org/projects/netconf/en/stable/testtool.html#notification-support -->
    <notification>
        <!-- delay 2 seconds after subscription request -->
        <delay>2</delay>
        <content><![CDATA[
        <notification xmlns="urn:ietf:params:xml:ns:netconf:notification:1.0">
            <eventTime>XXXX</eventTime>
            <device-event xmlns="test:device:simulator">
                <event-message>Event message</event-message>
            </device-event>
        </notification>
    ]]></content>
    </notification>
</notifications>