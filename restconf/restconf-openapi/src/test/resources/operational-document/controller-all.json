                  }
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/ietf-yang-library_modules-state"
                }
              }
            }
          }
        },
        "summary": "GET - Controller - datastore - data",
        "tags": [
          "Controller root"
        ],
        "parameters": [
          {
            "name": "content",
            "in": "query",
            "required": true,
