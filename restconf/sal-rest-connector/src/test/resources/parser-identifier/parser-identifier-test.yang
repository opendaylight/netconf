module parser-identifier {
  namespace "parser:identifier";
  prefix "parserid";
  yang-version 1;

  import parser-identifier-included { prefix pii; revision-date 2016-06-02; }

  revision 2016-06-02 {
    description
      "Initial revision.";
  }

  container cont1 {
    container cont2 {
      list list2 {
        key name;

        leaf name {
          type string;
        }

        uses group;
      }
    }
  }

  container cont3 {
    uses pii:group1;
  }

  grouping group {
    list list-in-grouping {
      key name;

      leaf name {
        type string;
      }
    }
  }
}