<?xml version="1.0" encoding="UTF-8"?>
<zdef937995056:data xmlns:zdef937995056="urn:ietf:params:xml:ns:netconf:base:1.0">
  <netconf-server xmlns="urn:ietf:params:xml:ns:yang:ietf-netconf-server">
    <listen>
      <endpoint>
        <ssh>
          <address>0.0.0.0</address>
          <port>830</port>
          <host-keys>
            <host-key>
              <public-key>ssh_host_rsa_key</public-key>
              <name>imported SSH key</name>
            </host-key>
          </host-keys>
        </ssh>
        <name>all-interfaces</name>
      </endpoint>
    </listen>
  </netconf-server>
  <enodeb-function xmlns="urn:rdns:com:ericsson:oammodel:ericsson-lrat-enb-adapter">
    <qci-table>
      <logical-channel-group>
        <id>2</id>
      </logical-channel-group>
      <qci-profile-operator-defined>
        <id>qci8</id>
        <priority>5</priority>
        <dscp>20</dscp>
        <resource-type>non-gbr</resource-type>
        <logical-channel-group-ref>2</logical-channel-group-ref>
        <qci>10</qci>
        <qci-subscription-quanta>100</qci-subscription-quanta>
      </qci-profile-operator-defined>
      <qci-profile-predefined>
        <id>qci9</id>
        <priority>5</priority>
        <dscp>20</dscp>
        <resource-type>non-gbr</resource-type>
        <logical-channel-group-ref>2</logical-channel-group-ref>
        <qci-subscription-quanta>100</qci-subscription-quanta>
      </qci-profile-predefined>
    </qci-table>
    <carrier-aggregation-function>
      <enhanced-selection-of-mimo-and-ca>false</enhanced-selection-of-mimo-and-ca>
      <selection-policy-ul-target-ratio>0</selection-policy-ul-target-ratio>
      <wait-for-blind-sel-s-cell-rep-less-ttt>600</wait-for-blind-sel-s-cell-rep-less-ttt>
      <s-cell-selection-mode>ack-sequential-scell-selection</s-cell-selection-mode>
      <selection-policy-ul-weighting>-1</selection-policy-ul-weighting>
      <wait-for-additional-s-cell-opportunity>10000</wait-for-additional-s-cell-opportunity>
      <wait-for-better-s-cell-rep>1000</wait-for-better-s-cell-rep>
      <s-cell-act-prohibit-timer>10</s-cell-act-prohibit-timer>
      <s-cell-schedule-sinr-thres>0</s-cell-schedule-sinr-thres>
      <dynamic-s-cell-selection-method>round-robin</dynamic-s-cell-selection-method>
      <s-cell-act-deact-ul-data-thresh-hyst>90</s-cell-act-deact-ul-data-thresh-hyst>
      <pdcch-enhanced-la-for-volte>false</pdcch-enhanced-la-for-volte>
      <ca-rate-adjust-coeff>10</ca-rate-adjust-coeff>
      <s-cell-deact-delay-timer>50</s-cell-deact-delay-timer>
      <wait-for-ca-opportunity>10000</wait-for-ca-opportunity>
      <ca-preemption-threshold>50</ca-preemption-threshold>
      <s-cell-act-deact-data-thres>100</s-cell-act-deact-data-thres>
      <use-absolute-prio-for-csp-eff-bw>false</use-absolute-prio-for-csp-eff-bw>
      <s-cell-act-deact-ul-data-thresh>100</s-cell-act-deact-ul-data-thresh>
      <s-cell-deact-prohibit-timer>200</s-cell-deact-prohibit-timer>
      <ca-preference>none</ca-preference>
      <effective-bw-impact-dl4-layer>150</effective-bw-impact-dl4-layer>
      <s-cell-pdcch-outer-loop-margin>100</s-cell-pdcch-outer-loop-margin>
      <ca-usage-limit>60</ca-usage-limit>
      <s-cell-deact-out-of-coverage-timer>100</s-cell-deact-out-of-coverage-timer>
      <four-layer-mimo-preferred>false</four-layer-mimo-preferred>
      <s-cell-act-deact-data-thres-hyst>99</s-cell-act-deact-data-thres-hyst>
    </carrier-aggregation-function>
    <enb-id>10</enb-id>
    <enodeb-plmn-id>
      <mnc-length>3</mnc-length>
      <mcc>10</mcc>
      <mnc>11</mnc>
    </enodeb-plmn-id>
    <load-balancing-function>
      <lb-ca-cap-hysteresis>20</lb-ca-cap-hysteresis>
      <lb-ca-threshold>800</lb-ca-threshold>
      <lb-threshold>30</lb-threshold>
      <lb-diff-ca-offset>100</lb-diff-ca-offset>
      <lb-ceiling>300</lb-ceiling>
      <lb-rate-offset-load-threshold>500</lb-rate-offset-load-threshold>
      <lb-rate-offset-coefficient>320</lb-rate-offset-coefficient>
    </load-balancing-function>
  </enodeb-function>
  <keystore xmlns="urn:ietf:params:xml:ns:yang:ietf-keystore">
    <private-keys>
      <private-key>
        <name>ssh_host_rsa_key</name>
      </private-key>
    </private-keys>
  </keystore>
</zdef937995056:data>
